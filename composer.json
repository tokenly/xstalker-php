{
    "name": "dweller/xstalker-php",
    "description": "Streams bitcoin transaction and block events to a beanstalk queue",
    "authors": [
        {
            "name": "Devon Weller",
            "email": "devon@tokenly.co"
        }
    ],
    "license": "MIT",
    "require": {
        "bitwasp/bitcoin-p2p": "dev-closeemitter",
        "packaged/figlet": "^0.0.1",
        "pda/pheanstalk": "^3.1",
        "vlucas/phpdotenv": "^2",
        "guzzlehttp/guzzle": "^5"
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tokenly/bitcoin-p2p-php"
        }
    ],

    "autoload": {
        "psr-4": {
            "XStalker\\": "src/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
